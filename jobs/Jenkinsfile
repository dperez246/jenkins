pipeline {
  agent { label 'master' }
  environment {
    appName = "variable" 
  }
  stages {
        stage('Prepare Build') {
           steps {
                  echo "build"
                }
            }
            stages {
                stage('Build') {
                    steps {
                        sh 'yarn install'
                    }
                }
                stage('Lint') {
                    steps {
                        sh 'yarn lint'
                    }
                }
            }
        }

  }
  post {
      always {          
          deleteDir()
           sh "echo 'fase always'"
      }
      success {
            sh "echo 'fase success'"
        }

      failure {
            sh "echo 'fase failure'"
      }
      
  }
}  




